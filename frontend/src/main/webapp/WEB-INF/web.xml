<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
        <servlet-name>GestorLogin</servlet-name>
        <servlet-class>net.rnvn.frontend.credenciales.GestorLogin</servlet-class>
</servlet>
<servlet-mapping>
        <servlet-name>GestorLogin</servlet-name>
        <url-pattern>/GestorLogin</url-pattern>
        <url-pattern>/login</url-pattern>
</servlet-mapping>

<servlet>
        <servlet-name>GestorRegistro</servlet-name>
        <servlet-class>net.rnvn.frontend.credenciales.GestorRegistro</servlet-class>
</servlet>
<servlet-mapping>
        <servlet-name>GestorRegistro</servlet-name>
        <url-pattern>/GestorRegistro</url-pattern>
        <url-pattern>/register</url-pattern>
</servlet-mapping>

<session-config>
    <session-timeout>
        30
    </session-timeout>
</session-config>

</web-app>

